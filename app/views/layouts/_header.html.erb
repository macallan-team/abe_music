<header class="container-fluid">
	<div class="row">
		<div class="col-sm-4">
		</div>
		<div class="col-sm-4 nav-center center">
			<%= link_to root_path do %><h1>AbeMusic</h1><% end %>
		</div>
		<div class="col-sm-4 nav-right">
			<div class="menu-btn">
				<span class="btn-icon">三</span>
			</div>
			<div class="login-status">
				<% if user_signed_in? %>
				<% else %>
					<%= link_to "ログイン",new_user_session_path %><br>
					<%= link_to "新規登録",new_user_registration_path %>
				<% end %>
			</div>
		</div>
	</div>
	<div class="row">
      <%= render partial: "layouts/menubox" %>
    </div>
</header>